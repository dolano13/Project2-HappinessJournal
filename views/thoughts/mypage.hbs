<h1 id="big"> {{currentUser.username}}</h1>

<div class="faves">
   <section id="dashboard-landing">
      <article class="dashpanel">
         <h4>Last journal entry:</h4>
      {{#if theDate}}
         <a href="/allPosts">{{theDate}}</a>
      {{/if}}
      </article>
{{#each userFaves}}
      <article class="dashpanel">
         <img src="{{this.pic}}" alt="">
         <form action="/dashboard/delete/{{this._id}}" method="post"> <button type="submit">🗑️</button> </form>
      </article>
{{/each}}
   </section>
</div>

